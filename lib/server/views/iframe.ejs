<!doctype html>
<html lang="<%= lang %>">
<head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <% if (favicons.length > 0) { %>
        <% favicons.forEach(item => { %>
            <%- item %>
        <% }) %>
    <% } else { %>
        <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <% } %>
    <style>
        body {
            margin: 0;
        }

        iframe {
            display: block;
            background: #ffffff;
            border: none;
            height: 100vh;
            width: 100vw;
        }
    </style>
</head>
<body>
<iframe src="<%= url %>"></iframe>
</body>
</html>
